{
  "id": null,
  "title": "SRE Test - Simple Queries",
  "tags": ["test"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Basic Service Status",
      "type": "stat",
      "targets": [
        {
          "expr": "up",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "HTTP Request Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(http_requests_total[5m])",
          "legendFormat": "{{service}} - {{status_code}}{{status}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      },
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 0}
    },
    {
      "id": 3,
      "title": "Probe Success",
      "type": "timeseries",
      "targets": [
        {
          "expr": "probe_success",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "gridPos": {"h": 6, "w": 24, "x": 0, "y": 6}
    }
  ]
}