          "legendFormat": "Frontend SLO: 99.9%",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 99,
          "max": 100,
          "thresholds": {
            "steps": [
              {"color": "red", "value": 99},
              {"color": "yellow", "value": 99.5},
              {"color": "green", "value": 99.9}
